Name='nav_items'>\n            <a href='https://github.com/mseaman26' target={\"blank\"} className='nav_link' >\n                <p>Visit my GitHub Page</p>\n            </a>\n            <a href='https://www.linkedin.com/in/michael-seaman-120a59250/' target={\"blank\"} className='nav_link'>\n                <p>Visit my LinkedIn</p>\n            </a>\n            <a href='https://medium.com/@mseaman26' target={\"blank\"} className='nav_link'>\n                <p>View my Medium Blog posts</p>\n            </a>\n            </div>\n        </div>\n    )\n}\nexport default Footer","/Users/michaelseaman/bootcamp/React-Portfolio-site/portfolio-site/src/components/Pages/Resume.js",["56"],"import React, { useState } from 'react';\nimport resume from '../../assets/images/Michael_Seaman2.png'\nimport resume2 from '../../assets/images/Michael_Seaman1.png'\nimport resumePDF from '../../assets/images/Michael_Seaman.pdf'\n\nfunction Resume() {\n    return(\n        <div id='resume_page'>\n            <h1 id='resume_header'>\n                If you'd like to download my resume, simply click on it!\n            </h1>\n            \n                <div id='resume_div'>\n                    <a href={resumePDF} download id='resume_a'>\n                    <img className='resume_pic' src={resume2} alt='Resume for Mike Seaman'/>\n                    <img className='resume_pic' src={resume} alt='Resume for Mike Seaman'/>\n                    </a>\n                </div>\n            \n        </div>\n     \n    )\n}\nexport default Resume","/Users/michaelseaman/bootcamp/React-Portfolio-site/portfolio-site/src/components/Pages/Contact.js",["57","58"],"import React, { useState } from 'react';\nimport { validateEmail } from '../../utils/helpers'\nimport emailjs from 'emailjs-com'\n\nfunction Contact() {\n\n  function sendEmail(e){\n    e.preventDefault();\n\n    emailjs.sendForm('Gmail', 'template_x04vxpm', e.target, 'u1xtN4l27HdhYjBd0')\n      .then((result) => {\n          console.log(result.text);\n      }, (error) => {\n          console.log(error.text);\n      });\n      e.target.reset()\n  }\n\n    const [email, setEmail] = useState('');\n    const [firstName, setFirstName] = useState('');\n    const [lastName, setLastName] = useState('');\n    const [message, setMessage] = useState('')\n    const [errorMessage, setErrorMessage] = useState('');\n\n    const handleInputChange = (e) => {\n        const { target } = e;\n        const inputType = target.name;\n        const inputValue = target.value;\n    \n\n        if (inputType === 'email') {\n          setEmail(inputValue);\n        } else if (inputType === 'firstName') {\n          setFirstName(inputValue);\n        } else if (inputType === 'lastName'){\n          setLastName(inputValue);\n        } else {\n          setMessage(inputValue)\n        }\n        //template_x04vxpm\n      };\n      const handleFormSubmit = (e) => {\n\n        e.preventDefault();\n        if (!validateEmail(email)) {\n          setErrorMessage('!!!Email is invalid');\n          return;\n        }\n        if(!firstName || !lastName){\n            setErrorMessage('!!!please provide a first and last name')\n            return\n        }\n        if(!message){\n          setErrorMessage('your message cannot be blank!')\n          return\n        }\n        let form = document.getElementById('contact_form')\n        emailjs.sendForm('service_603mx0m', 'template_x04vxpm', form, 'u1xtN4l27HdhYjBd0')\n        .then((result) => {\n          console.log(result.text);\n          }, (error) => {\n          console.log(error.text);\n          });\n\n        setFirstName('');\n        setLastName('');\n        setEmail('');\n        setErrorMessage('')\n        setMessage('')\n        alert(`message sent!`);\n    \n      };\n\n    return (\n        <div id='conact_page'>\n           \n            <div id='contact_form_div'>\n                <div className='contact_header_div'>\n                  <h1 id='contact_header'>\n                  Contact Michael Seaman At:\n                  </h1>\n                  <a id='email_mike' href='mailto:mseaman26@gmail.com' target='_blank'>mseaman26@gmail.com</a>\n                  <h1 id='contact_header'>\n                  Or Fill Out the Following Contact Form:\n                  </h1>\n                  \n                </div>\n                <br/>\n                <br/>\n                <form className=\"form\" id='contact_form'>\n                <input\n                value={email}\n                name=\"email\"\n                onChange={handleInputChange}\n                type=\"email\"\n                placeholder=\"email\"\n                className='contact_form_item'\n                /><br/>\n                <input\n                value={firstName}\n                name=\"firstName\"\n                onChange={handleInputChange}\n                type=\"text\"\n                placeholder=\"First Name\"\n                className='contact_form_item'\n                /><br/>\n                <input\n                value={lastName}\n                name=\"lastName\"\n                onChange={handleInputChange}\n                type=\"text\"\n                placeholder=\"Last Name\"\n                className='contact_form_item'\n                \n                />\n                <br/>\n                <input\n                value={message}\n                name=\"message\"\n                onChange={handleInputChange}\n                type=\"text\"\n                placeholder=\"Enter your message here\"\n                className='contact_form_item'\n                rows='4'\n                />\n                <br/>\n                <button type=\"button\" onClick={handleFormSubmit}>Submit</button>\n                </form>\n                {errorMessage && (\n                    <div>\n                    <p className=\"error-text\">{errorMessage}</p>\n                    </div>\n                )}\n            </div>\n         \n        </div>\n        \n    )\n}\nexport default Contact","/Users/michaelseaman/bootcamp/React-Portfolio-site/portfolio-site/src/components/projects/Projects.js",["59"],"/Users/michaelseaman/bootcamp/React-Portfolio-site/portfolio-site/src/components/projects/Project.js",["60","61"],"/Users/michaelseaman/bootcamp/React-Portfolio-site/portfolio-site/src/components/Pages/About/About.js",["62","63"],"import React, { useState } from 'react';\nimport mikePic from '../../../assets/images/mike.png'\nfunction About() {\n    return(\n        <div id='about_page'>\n            <img src={mikePic} id='mikePic'></img>\n            <div id='about_me'>\n                \n                <h1 id='about-header'>\n                    About Michael Seaman\n                    <br/>\n                    <br/>\n                   \n                    * * *\n                </h1>\n                \n                <p className='about_p'>\n                I grew up in the beautiful redwood tree forests of Mt. Tamalpias in Marin County, CA.  I’ve had a wide range of interests throughout my life, including classical piano, electronic music, surfing, hiking, and of course, coding.  My real introduction to coding began while I was studying music at UC Santa Cruz.  My favorite music professor, David Cope, taught classes and summer workshops in algorithmic computer music, all of which I attended.  While applying my interest in music to the field of coding, I discovered an aptitude and an enthusiasm for the type of problem-solving that coding entails.  I love debugging and making things work! I did a few independent projects after college, but I really began to code seriously when I enrolled in the UC Berkeley Extension Full-Stack Web Development Coding Bootcamp.  It was thrilling to re-engage with this usage of my mind and to learn more current tools.  I’m ready for a career in web development and I am really excited to continue my learning! I am passionate about and proficient in the following skills:\n                </p>\n                <ul>\n                    <li>\n                        <p className='about_p'>\n                            Front End Development\n                        </p>\n                    </li>\n                    <li>\n                        <p className='about_p'>\n                            Back End Development\n                        </p>\n                    </li>\n                    <li>\n                        <p className='about_p'>\n                            MERN\n                        </p>\n                    </li>\n                    <li>\n                        <p className='about_p'>\n                            Javascript/ES6\n                        </p>\n                    </li>\n                    <li>\n                        <p className='about_p'>\n                            HTML 5, CSS\n                        </p>\n                    </li>\n                    <li>\n                        <p className='about_p'>\n                            Relational Databases: SQL, MySQL, MongoDB, Mongoose, GraphQL\n                        </p>\n                    </li>\n                    <li>\n                        <p className='about_p'>\n                            Debugging, Problem-Solving\n                        </p>\n                    </li>\n                    <li>\n                        <p className='about_p'>\n                            Communication\n                        </p>\n                    </li>\n                    <li>\n                        <p className='about_p'>\n                            ...and Much More!\n                        </p>\n                    </li>\n                </ul>\n                \n            </div>\n                \n            \n        </div>\n            \n    )\n}\nexport default About","/Users/michaelseaman/bootcamp/React-Portfolio-site/portfolio-site/src/utils/helpers.js",[],{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","r