oject) => (\n            <div className='project-card' >\n                \n                <div className='project_title'>\n                    <h3 className='project_header'>\n                        {project.name}\n                    </h3>\n                </div>\n                <div className='project_image_div'>\n                    <img id={project.imageIndex} src={images[project.imageIndex] } className='project_image' alt={project.alt} onMouseOver={imageHoverHandler} onMouseOut={imageUnHoverHandler}/>\n                </div>                   \n            </div>\n        ))\n)\n}\n\nexport default Project","/Users/michaelseaman/bootcamp/React-Portfolio-site/portfolio-site/src/components/Pages/About/About.js",["61","62"],"/Users/michaelseaman/bootcamp/React-Portfolio-site/portfolio-site/src/utils/helpers.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":1,"column":17,"nodeType":"69","messageId":"70","endLine":1,"endColumn":25},{"ruleId":"67","severity":1,"message":"68","line":1,"column":17,"nodeType":"69","messageId":"70","endLine":1,"endColumn":25},{"ruleId":"67","severity":1,"message":"68","line":1,"column":17,"nodeType":"69","messageId":"70","endLine":1,"endColumn":25},{"ruleId":"67","severity":1,"message":"71","line":2,"column":10,"nodeType":"69","messageId":"70","endLine":2,"endColumn":23},{"ruleId":"67","severity":1,"message":"68","line":1,"column":17,"nodeType":"69","messageId":"70","endLine":1,"endColumn":25},{"ruleId":"67","severity":1,"message":"68","line":1,"column":17,"nodeType":"69","messageId":"70","endLine":1,"endColumn":25},{"ruleId":"72","severity":1,"message":"73","line":23,"column":9,"nodeType":"74","endLine":23,"endColumn":62},{"ruleId":"67","severity":1,"message":"68","line":1,"column":17,"nodeType":"69","messageId":"70","endLine":1,"endColumn":25},{"ruleId":"75","severity":1,"message":"76","line":9,"column":13,"nodeType":"74","endLine":9,"endColumn":72},"no-nati